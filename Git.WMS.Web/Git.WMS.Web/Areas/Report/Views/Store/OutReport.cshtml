@{
    Layout = null;
}

<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    @Html.Partial("_PageTitle")
</head>
<body class="fixed-top">

    @Html.Partial("_Header")

    <div id="container" class="row-fluid @ViewBag.MenuStatus">
        @Html.Partial("_Sidebar")

        <div id="body">
            <div id="widget-config" class="modal hide">
                <div class="modal-header">
                    <button data-dismiss="modal" class="close" type="button">×</button>
                    <h3>Widget Settings</h3>
                </div>
                <div class="modal-body">
                    <p>Here will be a configuration form</p>
                </div>
            </div>
            <div class="container-fluid">
                <div class="row-fluid">
                    <div class="span12">
                        @Html.Raw(ViewBag.NavMenu)
                    </div>
                </div>
                <div class="row-fluid">
                    <div class="span12">
                        <div class="widget box light-grey">
                            <div class="widget-title">
                                <h4><i class="icon-globe"></i>入库详细报表</h4>
                                <span class="tools">
                                    <a href="javascript:void(0);" class="icon-download" data-command="Excel"></a>
                                </span>
                            </div>
                            <div class="widget-body">

                                <div class="row-fluid">
                                    <form class="form-horizontal" action="javascript:void(0)">
                                        <div class="control-group">
                                            <div data-condition="search">
                                                产品名称:<input type="text" name="ProductName" class="input-medium" placeholder="请输入产品名称" />
                                                产品编号:<input type="text" name="BarCode" class="input-medium" placeholder="请输入产品编号" />
                                                客户:<input type="text" name="CusNum" class="input-medium" placeholder="请输入客户名称或编号" />
                                                起始时间:<input type="text" name="BeginTime" class="input-medium" placeholder="" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd'})" />-
                                                <input type="text" name="EndTime" class="input-medium" placeholder="" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd'})" />
                                                <a class="btn btn-info" href="javascript:void(0)" data-command="search"><i class="icon-search"></i>&nbsp;搜索</a>
                                            </div>
                                        </div>
                                    </form>
                                </div>

                                <table class="table table-striped table-bordered table-hover" id="tabList">
                                    <thead>
                                        <tr>
                                            <th>产品名称</th>
                                            <th>产品编号</th>
                                            <th>产品规格</th>
                                            <th>产品批次</th>
                                            <th>出库单号</th>
                                            <th>数量</th>
                                            <th>单价</th>
                                            <th>总价</th>
                                            <th>客户</th>
                                            <th>出库时间</th>
                                        </tr>
                                    </thead>
                                </table>

                                <div id="mypager" class="row-fluid" style="vertical-align:top;">

                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @Html.Partial("_Foot")
    <script src="/Theme/customer/Report/Git.Framework.Report.OutReport.js"></script>
    <script>
        jQuery(document).ready(function () {
            App.init();
            OutReport.Init();
        });
    </script>
</body>
</html>