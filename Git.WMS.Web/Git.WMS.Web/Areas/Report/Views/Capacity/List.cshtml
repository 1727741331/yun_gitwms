@{
    Layout = null;
}


<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    @Html.Partial("_PageTitle")
</head>
<body class="fixed-top">

    @Html.Partial("_Header")

    <div id="container" class="row-fluid @ViewBag.MenuStatus">
        @Html.Partial("_Sidebar")

        <div id="body">
            <div id="widget-config" class="modal hide">
                <div class="modal-header">
                    <button data-dismiss="modal" class="close" type="button">×</button>
                    <h3>Widget Settings</h3>
                </div>
                <div class="modal-body">
                    <p>Here will be a configuration form</p>
                </div>
            </div>
            <div class="container-fluid">
                <div class="row-fluid">
                    <div class="span12">
                        @Html.Raw(ViewBag.NavMenu)
                    </div>
                </div>
                <div class="row-fluid">
                    <div class="span12">
                        <div class="widget box light-grey">
                            <div class="widget-title">
                                <h4><i class="icon-globe"></i>库存容量</h4>
                                <span class="actions toolbar pull-right tools">
                                    <a style="color:#fff;" class="btn btn-info" href="javascript:void(0)" data-command="Excel"><i class="icon-download"></i>&nbsp;导出</a>
                                </span>
                            </div>
                            <div class="widget-body">
                                <div class="row-fluid">
                                    <form class="form-horizontal" action="javascript:void(0)">
                                        <div class="control-group">
                                            <div data-condition="search">
                                                仓库:<select class="input-medium" name="StorageNum">@Html.Raw(ViewBag.Storage)</select>
                                                <a class="btn btn-info" href="javascript:void(0)" data-command="search"><i class="icon-search"></i>&nbsp;搜索</a>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <div id="tabList" class="row-fluid"></div>
                                <div id="mypager" class="row-fluid" style="vertical-align:top;"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @Html.Partial("_Foot")
    <script src="/Theme/assets/plugins/mmGrid/src/mmGrid.js"></script>
    <script src="/Theme/customer/Report/Git.Framework.Report.Capacity.js"></script>
    <script>
        jQuery(document).ready(function () {
            App.init();
            CapacityManager.Init();
        });
    </script>
</body>
</html>