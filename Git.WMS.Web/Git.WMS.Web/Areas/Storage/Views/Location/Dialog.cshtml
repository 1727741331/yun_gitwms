@{
    Layout = null;
}
<div class="widget box light-grey">
    <div class="widget-body">
        <div class="dataTables_wrapper form-inline">
            <div class="row-fluid">
                <div class="span12 search">
                    <label>库位编号: <input type="text" name="LocalBarCode" class="input-small"></label>
                    <label>库位名称: <input type="text" name="LocalName" class="input-small"></label>
                    <label>仓库: 
                        <select class="input-medium" name="StorageNum" style="width: 130px;">
                            @Html.Raw(ViewBag.Storage)
                        </select>
                    </label>
                    <button class="btn btn-primary" data-command="Search">搜索</button>
                </div>
            </div>
            <div class="row-fluid" id="tabInfo"></div>
            <div id="myMinPager" class="row-fluid" style="vertical-align:top;"></div>
        </div>
    </div>
</div>